{
  "name": "hawleym-personal-site",
  "version": "0.0.1",
  "homepage": "https://hawleym.github.io/personal-site/",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hawleym/personal-site.git"
  },
  "scripts": {
    "start": "next",
    "lint": "eslint pages components *.config.js",
    "lint:fix": "eslint pages components *.config.js --fix",
    "build": "next build",
    "export": "next export",
    "predeploy": "rm -rf node_modules/.cache && npm run build && npm run export",
    "deploy": "gh-pages -t true -d out && rm -rf out/"
  },
  "dependencies": {
    "autoprefixer": "10.2.3",
    "gh-pages": "^3.1.0",
    "next": "latest",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "postcss": "8.2.4",
    "tailwindcss": "^2.0.2"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.18.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-nextjs": "^1.0.7",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-html": "^6.1.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "prettier": "^2.2.1"
  },
  "babel": {
    "presets": [
      "next/babel"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
